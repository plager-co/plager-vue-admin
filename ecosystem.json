{
  "apps" : {
    "name"        : "vuejs",
    "script"      : "webpack-dev-server --hot --inline --progress --config build/webpack.dev.conf.js",
    "watch"       : true,
    "instances"  : 1,
    "exec_mode"  : "cluster",
    "env": {
      "COMMON_VARIABLE": "true"
    },
    "env_production" : {
       "NODE_ENV": "production"
    },
    "env_develpomemt" : {
       "NODE_ENV": "development"
    }
  }
}
